#include<iostream>
#include<stack>
#include<string>
#include<cmath>
using namespace std;
int eval(int op1, int op2, char o) {
switch (o) {
case '+': return op1 + op2;
case '-': return op1 - op2;
case '*': return op1 * op2;
case '/': return op1 / op2;
case '^': return static_cast<int>(pow(op1, op2));
}
return 0;}
int evalPfx(string pfx) {
stack<int> s;
for (int i = pfx.length() - 1; i >= 0; i--) {
char c = pfx[i];
if (isdigit(c)) {
s.push(c - '0'); 
} else {
int op1 = s.top(); s.pop();
int op2 = s.top(); s.pop();
s.push(eval(op1, op2, c));  } }
return s.top();  }
int main() {
string pfx = "-+*234/87";
cout << "Prefix Evaluation Result: " << evalPfx(pfx) << endl;
return 0;}
