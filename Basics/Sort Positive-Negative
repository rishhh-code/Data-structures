#include<iostream>
using namespace std;
int main()
{
int arr[50];
int arr1[50];
int arr2[50];
int n,k=0,m=0;
cout<<"enter the no of elements in array:"<<endl;
cin>>n;
cout<<"enter the elements in array:"<<endl;
for(int i=0;i<n;i++)
{
cin>>arr[i];
}
for(int i=0;i<n;i++)
{
if(arr[i]<0)
{
arr1[k]=arr[i];
k++;
}
}
for(int i=0;i<n;i++)
{
if(arr[i]>=0)
{
arr2[m]=arr[i];
m++;
}
}
for(int i=0;i<k;i++)
{
cout<<arr1[i]<<" ";
}
for(int i=0;i<m;i++)
{
cout<<arr2[i]<<" ";
}
}
